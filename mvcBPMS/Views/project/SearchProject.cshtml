@model IEnumerable<mvcBPMS.Models.project>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SearchProject</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.project_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.standard_product_value)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.enter_date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.exit_date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.is_approved)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.approved_datatime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.contact.staff_id)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.project_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.standard_product_value)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.enter_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.exit_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.is_approved)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.approved_datatime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.contact.staff_id)
            </td>
            <td>
                @Html.ActionLink("项目详情", "projectDetails", new { id=item.id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
